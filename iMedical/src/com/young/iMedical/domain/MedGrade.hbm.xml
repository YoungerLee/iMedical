<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.young.iMedical.domain">
	<class name="MedGrade" table="med_grade">
		<id name="user_id" type="integer">
			<column name="user_id" sql-type="int" not-null="true"/>
		</id>
		<property name="med_id" type="integer">
			<column name="med_id" sql-type="int"/>
		</property>
		<property name="grade" type="double">
			<column name="grade" sql-type="double"/>
		</property>
	</class>
</hibernate-mapping>